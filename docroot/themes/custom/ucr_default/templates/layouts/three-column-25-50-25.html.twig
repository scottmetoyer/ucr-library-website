<div class="two-column grid-x grid-padding-x">
  <div class="top cell small-12">
    {{ content.top }}
  </div>
  <div class="column-1 cell small-12 medium-3">
    {{ content.left_column }}
  </div>
  <div class="column-2 cell small-12 medium-6">
    {{ content.main }}
  </div>
   <div class="column-3 cell small-12 medium-3">
    {{ content.right_column }}
  </div>
  <div class="bottom cell small-12">
    {{ content.bottom }}
  </div>
</div>