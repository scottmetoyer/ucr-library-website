{#
/**
 * @file
 * Default theme implementation to display a list of Eventbrite events.
 *
 * Available variables:
 *   - events
 *
 * @ingroup themeable
 */
#}
<div class="event-list">
    {% for event in events.events %}
        <img src="{{ event.logo.url }}" alt="{{ event.name.text }}"/>
        <h4>{{ event.name.text }}</h4>

        {% if event.is_series %}
        <strong>Multiple events</strong>
        {% endif %}
        <p>{{ event.start.local }}
            -
            {{ event.end.local }}</p>
        <p>
            <a target="_blank" href="{{ event.url }}">RSVP</a>
        </p>
        <p>{{ event.description.text }}</p>
        <hr>
    {% endfor %}
</div>